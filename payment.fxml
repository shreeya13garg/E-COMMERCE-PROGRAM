<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="417.0" prefWidth="610.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vendor.Payment">
   <children>
      <ImageView fitHeight="417.0" fitWidth="610.0" layoutY="2.0" pickOnBounds="true">
         <image>
            <Image url="@images2.jpeg" />
         </image>
      </ImageView>
      <Label layoutX="135.0" layoutY="274.0" text="FINAL PAYABLE AMOUNT">
         <font>
            <Font name="Arial Rounded MT Bold" size="16.0" />
         </font></Label>
      <Label fx:id="label" layoutX="380.0" layoutY="274.0" text="" />
      <TableView fx:id="product" layoutX="135.0" layoutY="52.0" opacity="0.7" prefHeight="200.0" prefWidth="362.0">
        <columns>
            <TableColumn fx:id="ID" prefWidth="66.0" text="ID" />
          <TableColumn fx:id="Name" prefWidth="76.0" text="Name" />
          <TableColumn fx:id="Cost" prefWidth="68.0" text="Cost" />
            <TableColumn fx:id="Quantity" minWidth="9.0" prefWidth="74.0" text="Quantity" />
            <TableColumn fx:id="S_id" prefWidth="77.0" text="S_Id" />
        </columns>
      </TableView>
      <Button fx:id="btn1" layoutX="145.0" layoutY="322.0" mnemonicParsing="false" onAction="#Show" opacity="0.93" text="Show Items" />
      <Label layoutX="75.0" layoutY="14.0" text="Press On Show Items to view the details of the products in your Cart-:">
         <font>
            <Font size="16.0" />
         </font></Label>
      <Button layoutX="400.0" layoutY="322.0" mnemonicParsing="false" onAction="#previous" opacity="0.86" text="View Previous Orders" />
      <Button layoutX="280.0" layoutY="322.0" mnemonicParsing="false" onAction="#Next" opacity="0.82" text="Place Order" />
   </children>
</AnchorPane>
