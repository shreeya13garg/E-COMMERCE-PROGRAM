                  <Font name="Arial Rounded MT Bold" size="19.0" />
               </font></Label>
            <Button layoutX="480.0" layoutY="69.0" mnemonicParsing="false" onAction="#add" text="Add Items to the cart" />
            <Button layoutX="630.0" layoutY="69.0" mnemonicParsing="false" onAction="#delete" text="Delete Items From the cart" />
            <ListView fx:id="list1" layoutX="20.0" layoutY="129.0" prefHeight="200.0" prefWidth="375.0" />
            <ListView fx:id="list2" layoutX="420.0" layoutY="129.0" prefHeight="200.0" prefWidth="375.0" />
